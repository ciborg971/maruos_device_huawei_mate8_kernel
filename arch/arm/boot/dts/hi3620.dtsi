			refclk_shareAXI: refclk@22 {
				compatible = "hisilicon,hi3620-clk-mux";
				#clock-cells = <0>;
				clocks = <&pll_usb &pll_peri>;
				hiword;
				clock-output-names = "rclk_shareAXI";
				hisilicon,clkmux-reg = <0x24 0x8000>;
				hisilicon,clkmux-table = <0 1>;
			};
			refclk_mmc1: refclk@23 {
				compatible = "hisilicon,hi3620-clk-mux";

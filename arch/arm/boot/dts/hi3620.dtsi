			};
			refclk_mmc1_parent: refclk@27 {
				compatible = "hisilicon,hi3620-clk-mux";
				#clock-cells = <0>;
				hiword;
				clocks = <&osc26m &div_mmc1>;
				clock-output-names = "rclk_mmc1_parent";
